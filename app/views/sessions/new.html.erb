<h1>Sign In</h1>

<form action="<%= session_url %>" method="post">
  <input
     name="authenticity_token"
     type="hidden"
     value="<%= form_authenticity_token %>">

  <label for="email">Email</label>
  <input
     id="email"
     name="email"
     type="text">
  <br>

  <label for="password">Password</label>
  <input
     id="password"
     name="password"
     type="password">
  <br>

  <input type="submit" value="Log in">
</form>

